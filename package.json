{
  "name": "remark-slate-transformer",
  "version": "0.9.0",
  "description": "remark plugin to transform remark syntax tree (mdast) to Slate document tree, and vice versa. Made for WYSIWYG markdown editor.",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.cjs",
      "default": "./lib/index.js"
    }
  },
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run",
    "tsc": "tsc -p . --noEmit",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "prepublishOnly": "rimraf lib && npm run build"
  },
  "dependencies": {
    "@types/mdast": "^4.0.4",
    "mdast-util-math": "^3.0.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.3.0",
    "@storybook/addon-docs": "^10.1.10",
    "@storybook/react-vite": "^10.1.10",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/unist": "^3.0.2",
    "github-markdown-css": "^4.0.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-is": "^19.2.3",
    "react-syntax-highlighter": "^15.5.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "rimraf": "^6.1.2",
    "rollup": "^4.54.0",
    "slate": "^0.120.0",
    "slate-history": "^0.113.1",
    "slate-react": "^0.120.0",
    "storybook": "^10.1.10",
    "typescript": "^5.9.0",
    "unified": "^11.0.5",
    "vite": "^7.3.0",
    "vitest": "^4.0.0"
  },
  "peerDependencies": {
    "unified": ">=11.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inokawa/remark-slate-transformer.git"
  },
  "keywords": [
    "unist",
    "remark",
    "mdast",
    "markdown",
    "slate",
    "react",
    "wysiwyg",
    "richtext",
    "editor"
  ],
  "author": "inokawa <stratoooo-taster@yahoo.co.jp> (https://github.com/inokawa/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inokawa/remark-slate-transformer/issues"
  },
  "homepage": "https://github.com/inokawa/remark-slate-transformer#readme"
}
