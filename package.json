{
  "name": "remark-slate-transformer",
  "version": "0.6.7",
  "description": "remark plugin to transform remark syntax tree (mdast) to Slate document tree, and vice versa. Made for WYSIWYG markdown editor.",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "tsc": "tsc -p . --noEmit",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "prepublishOnly": "rimraf lib && npm run build"
  },
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-typescript": "7.17.12",
    "@rollup/plugin-typescript": "8.3.2",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/addon-links": "6.4.22",
    "@storybook/react": "6.4.22",
    "@types/jest": "^27.4.0",
    "@types/mdast": "^3.0.10",
    "@types/slate_legacy": "npm:@types/slate@0.47.7",
    "@types/slate-react_legacy": "npm:@types/slate-react@0.22.9",
    "@types/unist": "2.0.6",
    "babel-jest": "27.4.6",
    "github-markdown-css": "4.0.0",
    "jest": "27.4.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "react-syntax-highlighter": "15.4.5",
    "remark-footnotes": "4.0.1",
    "remark-frontmatter": "4.0.1",
    "remark-gfm": "3.0.1",
    "remark-math": "5.1.1",
    "remark-parse": "10.0.1",
    "remark-stringify": "10.0.2",
    "rimraf": "3.0.2",
    "rollup": "2.72.0",
    "slate": "0.78.0",
    "slate_legacy": "npm:slate@0.47.9",
