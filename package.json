{
  "name": "remark-slate-transformer",
  "version": "0.2.0",
  "description": "remark plugin to transform remark synthax tree (mdast) to Slate document tree, and vice versa. Made for WYSIWYG markdown editor.",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "tsc": "tsc -p . --noEmit",
    "tsc:d": "tsc -p . --outDir lib -d --emitDeclarationOnly",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepublishOnly": "rimraf lib && npm run build && npm run tsc:d"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-typescript": "^6.0.0",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@types/jest": "^26.0.14",
    "@types/slate-react_legacy": "npm:@types/slate-react@0.22.9",
    "@types/slate_legacy": "npm:@types/slate@0.47.7",
    "@types/unist": "^2.0.3",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "github-markdown-css": "^4.0.0",
    "jest": "^26.4.2",
    "react-is": "^16.13.1",
    "react-syntax-highlighter": "^15.2.1",
    "remark-footnotes": "3.0.0",
    "remark-frontmatter": "3.0.0",
    "remark-gfm": "1.0.0",
    "remark-math": "^4.0.0",
    "remark-parse": "9.0.0",
    "remark-stringify": "9.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.0",
    "slate": "^0.59.0",
    "slate-history": "^0.59.0",
    "slate-react": "^0.59.0",
    "slate-react_legacy": "npm:slate-react@0.22.10",
    "slate_legacy": "npm:slate@0.47.9",
    "typescript": "^4.0.3",
    "unified": "^9.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inokawa/remark-slate-transformer.git"
  },
  "keywords": [
    "unist",
    "remark",
    "mdast",
    "markdown",
    "slate",
    "react",
    "wysiwyg",
    "richtext",
    "editor"
  ],
  "author": "inokawa <stratoooo-taster@yahoo.co.jp> (https://github.com/inokawa/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inokawa/remark-slate-transformer/issues"
  },
  "homepage": "https://github.com/inokawa/remark-slate-transformer#readme"
}
